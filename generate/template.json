  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#3F7F5F",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword.operator", "storage.modifier", "keyword.other"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.control",
        "storage.type",
        "punctuation.definition.directive",
        "keyword.language"
      ],
      "settings": {
        "foreground": "#7F0055",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#7F0055",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function.definition", "support.function"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#007D4E",
        "fontStyle": ""
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#0000C0",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#4F1919",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#2B00FF",
        "fontStyle": ""
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#3BAAFF"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#3BAAFF"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316BCD"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    }
  ]
}
