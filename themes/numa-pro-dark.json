    "titleBar.border": "#1e2838",
    "titleBar.activeBackground": "#1e2838",
    "titleBar.activeForeground": "#b5cbe9",
    "titleBar.inactiveBackground": "#1e2838",
    "titleBar.inactiveForeground": "#b5cbe9",
    "editorWidget.background": "#1e2838",
