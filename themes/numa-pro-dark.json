  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#71849f"
      },
      "name": "Comment"
    },
    {
      "scope": ["entity.name.constant", "variable.language"],
      "settings": {
        "foreground": "#53afff"
      },
      "name": "Constant"
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#7ac09e"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#7aa89d"
      }
    },
    {
      "scope": ["punctuation.accessor", "punctuation.separator.comma"],
      "settings": {
        "foreground": "#7EA7ED"
      }
    },
    {
      "scope": ["keyword.operator.symbole", "keyword.other.mark"],
      "name": "Clojure workaround; don't highlight these separately from their enclosing scope",
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#d593ff"
      },
      "name": "Entity"
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#e575c3"
      }
    },
    {
      "scope": [
        "entity.name.type.alias",
        "entity.name.type.interface",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#d593ff"
      }
    },
    {
      "scope": ["meta.type punctuation.accessor"],
      "settings": {
        "foreground": "#e575c3"
      }
    },
    {
      "scope": [
        "punctuation.definition.typeparameters",
        "meta.type.parameters punctuation.separator.comma"
      ],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": [
        "meta.field.declaration meta.definition.property entity.name",
        "meta.object-literal.key entity.name"
      ],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": [
        "meta.type.tuple entity.name.label",
        "punctuation.terminator.statement"
      ],
      "settings": {
        "foreground": "#677281"
      }
    },
    {
      "scope": ["variable.parameter.function"],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.name.tag support.class.component"],
      "settings": {
        "foreground": "#bdff95"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#7add3c"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ff6983"
      },
      "name": "Keyword"
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#ff6983"
      },
      "name": "Storage"
    },
    {
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#D593FF"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#7ac09e"
      },
      "name": "String"
    },
    {
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "name": "Ada workaround; don't highlight imports as strings",
      "scope": ["string.unquoted.import.ada"],
      "settings": {}
    },
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#ffa850"
      },
      "name": "Support"
    },
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": [
        "meta.class support.class",
        "meta.class entity.other.inherited-class",
        "meta.interface support.class",
        "meta.interface entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e575c3"
      }
    },
    {
      "scope": ["meta.property-name"],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#b5cbe9"
      },
      "name": "Variable"
    },
    {
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["variable.object.property"],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["invalid.broken"],
      "settings": {
        "fontStyle": "bold italic underline",
        "foreground": "#b31d28"
      },
      "name": "Invalid - Broken"
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "fontStyle": "bold italic underline",
        "foreground": "#b31d28"
      },
      "name": "Invalid – Deprecated"
    },
    {
      "scope": ["invalid.illegal"],
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#b31d28"
      },
      "name": "Invalid – Illegal"
    },
    {
      "scope": ["carriage-return"],
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#ff6983"
      },
      "name": "Carriage Return"
    },
    {
      "scope": ["invalid.unimplemented"],
      "settings": {
        "fontStyle": "bold italic underline",
        "foreground": "#b31d28"
      },
      "name": "Invalid - Unimplemented"
    },
    {
      "scope": ["message.error"],
      "settings": {
        "foreground": "#b31d28"
      }
    },
    {
      "scope": ["string source"],
      "settings": {
        "foreground": "#b5cbe9"
      },
      "name": "String embedded-source"
    },
    {
      "scope": ["string variable"],
      "settings": {
        "foreground": "#53afff"
      },
      "name": "String variable"
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#53afff"
      },
      "name": "Support.constant"
    },
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#b5cbe9"
      },
      "name": "Support.variable"
    },
    {
      "scope": ["meta.module-reference"],
      "settings": {
        "foreground": "#53afff"
      },
      "name": "meta module-reference"
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#735c0f"
      },
      "name": "Markup.list"
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#53afff"
      },
      "name": "Markup.heading"
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#20c173"
      },
      "name": "Markup.quote"
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b5cbe9"
      },
      "name": "Markup.italic"
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b5cbe9"
      },
      "name": "Markup.bold"
    },
    {
      "scope": ["markup.raw"],
      "settings": {
        "foreground": "#53afff"
      },
      "name": "Markup.raw"
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#b31d28"
      },
      "name": "Markup.deleted"
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#20c173"
      },
      "name": "Markup.inserted"
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "foreground": "#ffa850"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "scope": ["meta.diff.range"],
      "settings": {
        "foreground": "#d593ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diff.header"],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#53afff"
      },
      "name": "Meta.separator"
    },
    {
      "scope": ["meta.brace.square"],
      "settings": {
        "foreground": "#92dd63"
      }
    },
    {
      "name": "Output",
      "scope": ["meta.output"],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "scope": ["brackethighlighter.unmatched"],
      "settings": {
        "foreground": "#b31d28"
      }
    },
    {
      "scope": ["sublimelinter.mark.error"],
      "settings": {
        "foreground": "#b31d28"
      }
    },
    {
      "scope": ["sublimelinter.mark.warning"],
      "settings": {
        "foreground": "#ffa850"
      }
    },
    {
      "scope": ["sublimelinter.gutter-mark"],
      "settings": {
        "foreground": "#959da5"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#7ea7ed",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "meta.function-call support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#53afff"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#20c173"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "keyword.operator.ternary",
        "keyword.operator.expression",
        "keyword.operator.optional",
        "punctuation.separator.label",
        "punctuation.separator.key-value",
        "meta.type storage.modifier"
      ],
      "settings": {
        "foreground": "#ff6983",
        "fontStyle": ""
      },
      "name": "Keyword"
    },
    {
      "scope": ["keyword.operator.expression.in"],
      "settings": {
        "foreground": "#20c173"
      }
    },
    {
      "scope": ["keyword.operator.type"],
      "settings": {
        "foreground": "#FFA850"
      }
    },
    {
      "scope": [
        "meta.objectliteral punctuation.definition.block",
        "meta.function.expression meta.objectliteral punctuation.definition.block",
        "meta.method.declaration.ts meta.block.ts meta.objectliteral punctuation.definition.block.ts",
        "meta.object.member keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#ff8b4d"
      }
    },
    {
      "scope": [
        "meta.object.type punctuation.definition.block",
        "meta.objectliteral meta.object.type punctuation.definition.block",
        "meta.function.expression punctuation.definition.block",
        "meta.method.declaration.ts meta.block.ts punctuation.definition.block.ts",
        "meta.objectliteral meta.method.declaration meta.block punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#b5cbe9"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#55719f"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7ea7ed"
      }
    },
    {
      "scope": ["variable.parameter", "entity.name.label"],
      "settings": {
        "foreground": "#d2a6ed"
      }
    },
    {
      "scope": ["meta.tag.attributes keyword.operator.assignment"],
      "settings": {
        "foreground": "#7ea7ed"
      }
    },
    {
      "scope": [
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#99655c"
      }
    }
  ]
}
