    "scrollbar.shadow": "#1e2838",
    "activityBar.border": "#1e2838",
    "activityBar.background": "#1e2838",
    "activityBar.foreground": "#b5cbe9",
    "activityBarBadge.background": "#1e2838",
    "statusBar.border": "#1e2838",
